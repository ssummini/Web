	<h1>[문제1] 2단 ~ 9단까지 구하시오.</h1>
	<div id="gugudan"></div>
<script type="text/javascript">
	for(let x=1; x<10; x++){
		let line = "";
		
		for(let dan=2; dan<10; dan++){
			line += `${dan} * ${x} = ${dan*x}`;
		}
		
		document.getElementById("gugudan").innerHTML += line + "<br>";
	}
</script>
