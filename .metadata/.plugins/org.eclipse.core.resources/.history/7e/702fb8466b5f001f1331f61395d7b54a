<script type="text/javascript">
	//concat - 배열 합치는 역할
	let arr1 = [10, 20, 30];
	let arr2 = ["사과", "딸기", "바나나"];
	let arr3 = arr1.concat();
	
	document.write("arr1 = " + arr1);
	document.write("<br>");
	document.write(`arr3 = ${arr3}`);
</script>
