<body>
	<h1>[문제3] 로또</h1>
<script type="text/javascript">
	let lottoNumbers = [];
	
	for(let i=0; i<6; i++){
		let number;
		let isSame;
		
		do{
			number = parseInt(Math.random() * 45 + 1);
			isSame = false;
			for(let j=0; j<lottoNumbers.length; j++){
				if(number == lottoNumbers[i]){
					isSame = true;
					break;
				}
			}
			
		}while(isSame)
			
		lottoNumbers.push(number);
	}
</script>
</body>
