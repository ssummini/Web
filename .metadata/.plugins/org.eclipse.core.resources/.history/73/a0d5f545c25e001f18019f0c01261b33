// 빈 객체로 생성
var calc = {}

//멤버변수 추가
 calc.x = 0;
 calc.y = 0;
 calc.sum = 0;
 calc.sub = 0;
 
 //멤버함수(자바에서는 메서드)
calc.setValue = function(x, y){
	this.x = x;
	this.y = y;
}

calc.plus = function(){
	return x + y;
}

calc.minus = function(){
	return x - y;
}

calc.result = function(){
	sum = plus;
	sub = minus;
	
	document.write('덧셈 = ' + this.sum);
	document.write('뺄셈 = ' + this.sub);
}